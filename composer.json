{
    "name": "l37sg0/architecture",
    "description": "Exercising on 'The Clean Architecture in PHP' by Kristopher Wilson.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "L37sg0\\Architecture\\": "src/",
            "Application\\": "module/Application/src/"
        }
    },
    "authors": [
        {
            "name": "Petar Ivanov",
            "email": "petar.ivanov@l37sg0.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "peridot-php/peridot-watcher-plugin": "dev-master",
        "peridot-php/peridot-prophecy-plugin": "dev-master"
    },
    "require": {
        "vnn/keyper": "dev-master",
        "laminas/laminas-dependency-plugin": "*",
        "laminas/laminas-component-installer": "*",
        "laminas/laminas-development-mode": "*",
        "laminas/laminas-mvc": "*",
        "laminas/laminas-db": "*",
        "laminas/laminas-mvc-form": "*",
        "laminas/laminas-json": "*",
        "laminas/laminas-log": "*",
        "laminas/laminas-mvc-i18n": "*",
        "laminas/laminas-mvc-plugins": "*",
        "laminas/laminas-mvc-middleware": "*",
        "laminas/laminas-session": "*",
        "laminas/laminas-di": "*",
        "laminas/laminas-hydrator": "*",
        "laminas/laminas-mvc-plugin-flashmessenger": "*"

    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true,
            "laminas/laminas-component-installer": true
        }
    }
}
