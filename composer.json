{
    "name": "l37sg0/architecture",
    "description": "Exercising on 'The Clean Architecture in PHP' by Kristopher Wilson.",
    "type": "library",
    "authors": [
      {
        "name": "Petar Ivanov",
        "email": "petar.ivanov@l37sg0.com"
      }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/orm": "^2.13",
        "symfony/yaml": "^5.4",
        "vnn/keyper": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "L37sg0\\Architecture\\": "core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
